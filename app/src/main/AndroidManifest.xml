<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.company.sandeep.zipcodesearch">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />


    <application
        android:allowBackup="true"
        android:icon="@drawable/icon"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:name="android.support.multidex.MultiDexApplication">
        <meta-data
            android:name="com.google.android.maps.v2.API_KEY"
            android:value="AIzaSyCltPEe3gOlpERs5K17Wx0guCVpF7DcWzQ" />
        <meta-data
            android:name="com.google.android.gms.version"
            android:value="@integer/google_play_services_version" />
        <meta-data
            android:name="io.fabric.ApiKey"
            android:value="7f1add120f35dc90cea9d66f058f4fa646ac87c9" />

        <activity
            android:name="com.company.sandeep.zipcodesearch.MainActivity"
            android:configChanges="keyboardHidden|screenLayout|screenSize|locale|orientation">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>


        <activity
            android:name="com.company.sandeep.zipcodesearch.Mylocation"
            android:parentActivityName="com.company.sandeep.zipcodesearch.MainActivity"></activity>

        <activity
            android:name="com.company.sandeep.database.MyShowHistory"
            android:parentActivityName="com.company.sandeep.zipcodesearch.MainActivity" />

        <activity
            android:name="com.company.sandeep.zipcodesearch.TimeZoneActivity"
            android:parentActivityName="com.company.sandeep.zipcodesearch.MainActivity"></activity>

    </application>

</manifest>